<!-- views/index.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>Admin User Details</title>
</head>
<body>
  <h2>Admin User Details</h2>
  <ul>
    <li> FirstName : <%= user.FirstName %>   </li>
    <li> LastName : <%= user.LastName %>   </li>
    <li> Email :  <%= user.Email %>   </li>
    <li> IsAdmin : <%= user.IsAdmin %>   </li>
    <li> RoleId : <%= user.RoleId %>   </li>
  </ul>

   <h2>User Details</h2>
  <ul>
    <% users.forEach(user => { %>
    <li> FirstName : <%= user.FirstName %>   </li>
    <li> LastName : <%= user.LastName %>   </li>
    <li> Email :  <%= user.Email %>   </li>
    <li> IsAdmin : <%= user.IsAdmin %>   </li>
    <li> RoleId : <%= user.RoleId %>   </li>
    <% }) %>
    <br>
  </ul>
</body>
</html>
